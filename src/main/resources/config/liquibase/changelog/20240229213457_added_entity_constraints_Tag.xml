<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Tag.
    -->
    <changeSet id="20240229213457-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tag_id"
                                 baseTableName="rel_tag__route"
                                 constraintName="fk_rel_tag__route__tag_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"/>

        <addForeignKeyConstraint baseColumnNames="route_id"
                                 baseTableName="rel_tag__route"
                                 constraintName="fk_rel_tag__route__route_id"
                                 referencedColumnNames="id"
                                 referencedTableName="route"/>

        <addForeignKeyConstraint baseColumnNames="tag_id"
                                 baseTableName="rel_tag__app_user"
                                 constraintName="fk_rel_tag__app_user__tag_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"/>

        <addForeignKeyConstraint baseColumnNames="app_user_id"
                                 baseTableName="rel_tag__app_user"
                                 constraintName="fk_rel_tag__app_user__app_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>
    </changeSet>
</databaseChangeLog>
